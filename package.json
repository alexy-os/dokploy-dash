{
  "name": "@dokploy/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Dokploy API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "generate:types": "openapi-typescript openapi.json -o src/types/dokploy.d.ts",
    "generate:sdk": "tsx scripts/generate-sdk.ts",
    "generate": "npm run generate:types && npm run generate:sdk",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "dokploy",
    "sdk",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openapi-fetch": "0.12.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "openapi-typescript": "^7.4.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
